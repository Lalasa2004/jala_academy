// Class Definition
class Person {
    // Instance variable
    String name;

    // Constructor
    Person(String name) {
        this.name = name;
    }

    // Method to print name
    void printName() {
        System.out.println("My name is " + name);
    }
}

public class Main {
    public static void main(String[] args) {
        // Creating an object of the class
        Person person = new Person("Alekhya");

        // Calling the method from the object
        person.printName();  // Method signature: void printName()
    }
}

