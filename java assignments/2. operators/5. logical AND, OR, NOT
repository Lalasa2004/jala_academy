public class Main {

    public static void main(String[] args) {
        boolean a = true, b = false;

        // Logical AND
        if (a && b) {
            System.out.println("Logical AND: Both are true.");
        } else {
            System.out.println("Logical AND: One or both are false.");
        }

        // Logical OR
        if (a || b) {
            System.out.println("Logical OR: At least one is true.");
        } else {
            System.out.println("Logical OR: Both are false.");
        }

        // Logical NOT
        System.out.println("Logical NOT of a: " + !a);
        System.out.println("Logical NOT of b: " + !b);
    }
}
