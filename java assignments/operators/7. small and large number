public class Main {

    public static void main(String[] args) {
        int num1 = 15, num2 = 10;

        // Find smaller number
        int smaller = (num1 < num2) ? num1 : num2;
        System.out.println("Smaller number: " + smaller);

        // Find larger number
        int larger = (num1 > num2) ? num1 : num2;
        System.out.println("Larger number: " + larger);
    }
}
