public class Main {
    // Global variable
    static String name = "Global Name";

    public static void main(String[] args) {
        // Local variable with the same name
        String name = "Local Name";
        
        // Printing the local variable
        System.out.println("Inside main method, Local name: " + name);
        
        // Printing the global variable
        System.out.println("Global name: " + Main.name);
    }
}
