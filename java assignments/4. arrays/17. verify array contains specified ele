public class Main {
    // Function to verify if the array contains two specified elements
    static boolean containsTwoElements(int[] arr, int el1, int el2) {
        boolean containsFirst = false;
        boolean containsSecond = false;
        for (int num : arr) {
            if (num == el1) containsFirst = true;
            if (num == el2) containsSecond = true;
        }
        return containsFirst && containsSecond;
    }

    public static void main(String[] args) {
        int[] numbers = {10, 12, 23, 34};
        System.out.println("Contains 12 and 23: " + containsTwoElements(numbers, 12, 23));
    }
}
